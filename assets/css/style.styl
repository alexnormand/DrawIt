@import 'normalize'

body
  background: #E7E9EB 
  padding: 0 50px 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  color: #666
a
  color: #00B7FF

#main
  background: white
  box-shadow: 0 0 10px #888
  width: 800px
  margin: auto
  padding: 10px
  overflow: hidden

  ol
    float: left
    margin-right: 20px
    width: 100px

    li
      text-overflow: ellipsis
      white-space:nowrap
      overflow:hidden
      list-style-position:inside


.mainTitle
  color: #A51;
  font-family: Satisfy
  font-size: 80px   
  margin: 0
  text-align: center
  text-shadow: 0 1px 2px #010202


button
  display: inline-block
  margin: 5px 0 0 5px

#canvas 
  border: 1px dashed #DDD
  display: inline-block
  float: left
  height: 400px
  margin: 10px
  width: 600px


footer
  text-align: center
  margin-top: 20px


